pipeline {
   agent any

   tools {
  maven 'Maven 3.6.3'
}

   stages {
     
     stage('code checkout') {
        steps {
        git credentialsId: 'git', url: 'https://github.com/Devops2272/New-Maven-Branch.git'
        
        }
     }
   
     stage('Generate Build Artifact') {
        steps {
        sh 'mvn clean package'
        
        }
     }
   
      /*stage('Sonar scan') {
        steps {
        sh 'mvn sonar:sonar'
        
        }
     }*/
    
    stage('mail') {
       post {
         success {
             mail to: 'chinmaya.devops2272@gmail.com',
             subject: "Build Succes: ${currentBuild.fullDisplayName}",
             body: "SUCCESSFUL: Job NTE_Build Check console output at  ${env.BUILD_URL}"
  }
}
}   
    
    
   
}
}
